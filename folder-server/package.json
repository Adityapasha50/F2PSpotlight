  "main": "index.js",
  "scripts": {
    "test": "jest --coverage --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageReporters=text --coverageReporters=html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Adityapasha50/F2PSpotlight.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Adityapasha50/F2PSpotlight/issues"
  },
  "homepage": "https://github.com/Adityapasha50/F2PSpotlight#readme",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.3.1",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "google-auth-library": "^9.15.1",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.0",
    "sequelize": "^6.37.7",
    "stripe": "^18.2.0",
    "supertest": "^7.1.1"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.3"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": ["./__tests__/setup.js"],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "text-summary",
      "html",
      "lcov"
    ],
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/__tests__/**",
      "!**/migrations/**",
      "!**/seeders/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
